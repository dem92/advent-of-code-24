    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch main",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "args": ["${input:enterTask}"]
          }
    ],
    "inputs": [
      {
        "id": "enterTask",
        "description": "Enter the task to run",
        "type": "promptString"
      }],
    "dlvFlags": ["--check-go-version=false"]
}